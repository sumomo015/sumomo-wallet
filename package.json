    "eslint": "^9.8.0",
    "happy-dom": "^14.12.3",
    "npm-check-updates": "^16.14.20",
    "nuxt-security": "^1.4.3",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^5.3.5",
    "vitest": "^2.0.4",
